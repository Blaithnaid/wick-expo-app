{
	"expo": {
		"name": "Wick",
		"slug": "wick-expo-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "wickapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#2E3443"
		},
		"ios": {
			"supportsTablet": true,
			"userInterfaceStyle": "automatic",
			"bundleIdentifier": "com.blaithnaid.wick-expo-app"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#2E3443"
			},
			"userInterfaceStyle": "automatic",
			"package": "com.blaithnaid.wickapp"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-image-picker",
				{
					"photosPermission": "Wick uses your gallery exclusively to select profile pictures."
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
